    "url": "https://github.com/Ashu-tosh-Kr/e-commerce-v1/issues"
  },
  "homepage": "https://github.com/Ashu-tosh-Kr/e-commerce-v1#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "mongoose": "^6.0.5"
  },
  "devDependencies": {
    "concurrently": "^6.2.1",
    "nodemon": "^2.0.12"
